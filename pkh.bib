file = {::},
journal = {Journal of Biomolecular Structure {\&} Dynamics},
number = {1},
pages = {123--135},
title = {{Molecular Dynamics and Docking Studies on Cardiac Troponin C}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21696229 papers2://publication/uuid/61706154-F931-4C95-8705-C51F93777CFD},
volume = {29},
year = {2011}
