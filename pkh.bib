title = {{A Mathematical Model for the Hard Sphere Repulsion in Ionic Solutions}},
url = {http://projecteuclid.org/euclid.cms/1305034462 papers2://publication/uuid/CD784D19-62A7-47BC-A2DC-FE5862B6A1DD},
