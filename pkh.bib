number = {28},
pages = {20897--20903},
title = {{Metal binding properties of recombinant rat parvalbumin wild-type and F102W mutant.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=8407923{\&}retmode=ref{\&}cmd=prlinks papers2://publication/uuid/9B041FB2-889B-497E-BF71-B2EBE955148C},
volume = {268},
