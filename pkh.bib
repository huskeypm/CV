title = {{Ca2+/calmodulin-dependent protein kinase modulates cardiac ryanodine receptor phosphorylation and sarcoplasmic reticulum Ca2+ leak in heart failure}},
url = {http://circres.ahajournals.org/content/97/12/1314.short papers2://publication/doi/10.1161/01.RES.0000194329.41863.89},
