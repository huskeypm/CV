publisher = { MIT Press  238 Main St., Suite 500, Cambridge, MA 02142-1046 USA journals-info@mit.edu  },
title = {{Eigenfaces for Recognition}},
url = {http://www.mitpressjournals.org/doi/abs/10.1162/jocn.1991.3.1.71},
volume = {3},
year = {1991}
}
