pages = {608--615},
title = {{Mechanisms of [Ca2+](i) transient decrease in cardiomyopathy of db/db type 2 diabetic mice}},
url = {http://diabetes.diabetesjournals.org/content/55/3/608.short papers2://publication/uuid/63163D24-48D3-4472-B6B3-79C1AAA7721D},
