file = {::},
journal = {Circulation Research},
number = {5},
pages = {415--427},
publisher = {Am Heart Assoc},
title = {{Subcellular [Ca2+](i) gradients during excitation-contraction coupling in newborn rabbit ventricular myocytes}},
