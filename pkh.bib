file = {::},
journal = {The Journal of general physiology},
number = {2},
pages = {119--131},
publisher = {Rockefeller Univ Press},
title = {{Allosteric regulation of Na/Ca exchange current by cytosolic Ca in intact cardiac myocytes}},
url = {http://links.isiglobalnet2.com/gateway/Gateway.cgi?GWVersion=2{\&}SrcAuth=mekentosj{\&}SrcApp=Papers{\&}DestLinkType=FullRecord{\&}DestApp=WOS{\&}KeyUT=000167608000003 papers2://publication/uuid/71E4167F-ECFC-4BFF-876C-162184750C27},
