author = {Bond, J M and Chacon, E and Herman, B and Lemasters, J J},
file = {::},
journal = {American Journal of Physiology},
number = {1 Pt 1},
pages = {C129--37},
title = {{Intracellular pH and Ca2+ homeostasis in the pH paradox of reperfusion injury to neonatal rat cardiac myocytes.}},
