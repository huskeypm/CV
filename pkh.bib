file = {::},
journal = {Biophysical Journal},
number = {7},
pages = {1616--1625},
publisher = {Elsevier},
title = {{A Biophysical Model of the Mitochondrial ATP-Mg/P(i) Carrier.}},
url = {http://pubget.com/paper/23062354?institution=ucsd.edu papers2://publication/doi/10.1016/j.bpj.2012.08.050},
volume = {103},
year = {2012}
}
