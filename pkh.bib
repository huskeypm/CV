file = {::},
journal = {American Journal Of Physiology-Heart And Circulatory Physiology},
title = {{Effects of increased systolic Ca2+ and phospholamban phosphorylation during  -adrenergic stimulation on Ca2+ transient kinetics in cardiac myocytes}},
